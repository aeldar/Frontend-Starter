{
  "projectType": "library",
  "root": "libs/feature/products",
  "sourceRoot": "libs/feature/products/src",
  "prefix": "starter",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/feature/products"],
      "options": {
        "jestConfig": "libs/feature/products/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/feature/products/src/**/*.ts", "libs/feature/products/src/**/*.html"]
      }
    }
  },
  "tags": ["type:feat"]
}
